{
    "name": "plesk/ext-wappspector",
    "description": "Analyze the file structure of a web hosting server and identify the frameworks and CMS used in the websites hosted on it.",
    "type": "project",
    "license": "Apache-2.0",
    "autoload": {
        "psr-4": {
            "PleskExt\\Wappspector\\": "modules/wappspector/library/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PleskExt\\Wappspector\\": "src/plib/library/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/plesk/wappspector.git"
        }
    ],
    "require": {
        "php": "^7.1|^8.0",
        "plesk/wappspector": "dev-main"
    },
    "require-dev": {
        "plesk/pm-api-stubs": "^18.0"
    },
    "config": {
        "platform": {
            "php": "8.0.28"
        },
        "vendor-dir": "src/plib/vendor"
    }
}
